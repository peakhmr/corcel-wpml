{
    "name": "peakhmr/wpml",
    "description": "The WordPress Multilingual [WPML] Plugin for Corcel/Laravel",
    "homepage": "https://github.com/peakhmr/wpml",
    "type": "laravel",
    "authors": [
        {
            "name": "Socheat",
            "email": "socheatsok78@gmail.com",
            "homepage": "https://socheat.net/"
        }
    ],
    "require": {
        "php": ">=7.1.3",
        "jgrossi/corcel": "v2.6.*",
        "corcel/acf": "1.*",
        "illuminate/database": "5.6.*",
        "illuminate/filesystem": "5.6.*",
        "illuminate/support": "5.6.*",
        "fzaninotto/faker": "^1.6",
        "hautelook/phpass": "0.3.4",
        "thunderer/shortcode": "^0.6.2"
    },
    "require-dev": {
        "orchestra/testbench": "3.6.x@dev",
        "orchestra/database": "3.6.x@dev",
        "phpunit/phpunit": "~7.0",
        "symfony/thanks": "^1.0",
        "codeclimate/php-test-reporter": "dev-master"
    },
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Wpml\\": "src/"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true
    }
}
